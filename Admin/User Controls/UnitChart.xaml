<UserControl x:Class="CarSalesSystem.Admin.User_Controls.UnitChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Orientation="Horizontal">
        <Border Width="12" Height="12" Background="#0074ff" CornerRadius="3"></Border>
        <TextBlock Text="Car" VerticalAlignment="Center" Margin="7 0 20 0" Foreground="#6c879a"></TextBlock>

        <Border Width="12" Height="12" Background="PaleVioletRed" CornerRadius="3"></Border>
        <TextBlock Text="Revenue" VerticalAlignment="Center" Margin="7 0 20 0" Foreground="#6c879a"></TextBlock>
    </StackPanel>
</UserControl>
